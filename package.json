{
  "name": "langchain",
  "version": "0.0.5",
  "description": "Typescript bindings for langchain",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "agents.js",
    "agents.d.ts",
    "tools.js",
    "tools.d.ts",
    "chains.js",
    "chains.d.ts",
    "embeddings.js",
    "embeddings.d.ts",
    "llms.js",
    "llms.d.ts",
    "prompts.js",
    "prompts.d.ts",
    "vectorstores.js",
    "vectorstores.d.ts",
    "text_splitter.js",
    "text_splitter.d.ts",
    "memory.js",
    "memory.d.ts",
    "document.js",
    "document.d.ts"
  ],
  "scripts": {
    "build": "tsc --declaration --outDir dist/ && node create-entrypoints.js",
    "lint": "eslint .",
    "doc": "typedoc",
    "lint:fix": "yarn lint --fix",
    "precommit": "tsc --noEmit && lint-staged",
    "clean": "rm -rf dist/ && node create-entrypoints.js clean",
    "prepack": "yarn build",
    "test": "jest",
    "prepare": "husky install"
  },
  "author": "Langchain",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@jest/globals": "^29.4.2",
    "@tsconfig/recommended": "^1.0.2",
    "@types/node-fetch": "2",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "babel-jest": "^29.4.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "hnswlib-node": "^1.2.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "lint-staged": "^13.1.1",
    "openai": "^3.1.0",
    "prettier": "^2.8.3",
    "serpapi": "^1.1.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.25",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "exponential-backoff": "^3.1.0",
    "expr-eval": "^2.0.2",
    "node-fetch": "2",
    "yaml": "^2.2.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --cache --fix"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}